components:
  schemas:
    Appointment:
      type: object
      required:
        - Operator
        - Assigee
        - IntakeDate
        - ReleaseDate
      properties:
        Id:
          type: integer
          format: int32
        Operator:
          type: integer
          format: int32
        Assigee:
          type: integer
          format: int32
        Status:
          type: integer
          format: int32
        SalesOrder:
          type: integer
          format: int32
        IntakeDate:
        ReleaseDate:
        Issue:
          type: string
          minLength: 0
          maxLength: 755
        Customer:
          type: integer
          format: int32
    AppointmentFilterOptions:
      type: object
      properties:
        $filter:
          type: object
          properties:
            equals:
              type: object
              properties:
                Id:
                  oneOf:
                    - type: integer
                      format: int32
                    - type: array
                      items:
                        type: integer
                        format: int32
                Operator:
                  oneOf:
                    - type: integer
                      format: int32
                    - type: array
                      items:
                        type: integer
                        format: int32
                Assigee:
                  oneOf:
                    - type: integer
                      format: int32
                    - type: array
                      items:
                        type: integer
                        format: int32
                Status:
                  oneOf:
                    - type: integer
                      format: int32
                    - type: array
                      items:
                        type: integer
                        format: int32
                SalesOrder:
                  oneOf:
                    - type: integer
                      format: int32
                    - type: array
                      items:
                        type: integer
                        format: int32
                IntakeDate:
                  oneOf:
                ReleaseDate:
                  oneOf:
                Issue:
                  oneOf:
                    - type: string
                      minLength: 0
                      maxLength: 755
                    - type: array
                      items:
                        type: string
                        format: double
                        minLength: 0
                        maxLength: 755
                Customer:
                  oneOf:
                    - type: integer
                      format: int32
                    - type: array
                      items:
                        type: integer
                        format: int32
            notEquals:
              type: object
              properties:
